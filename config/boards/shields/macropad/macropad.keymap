        compatible = "zmk,keymap";

        MAIN_layer {
            bindings = <
                &kp N7      &kp N7      &kp N7      &kp N7      &kp N7
                &kp N7      &kp N7      &kp N7      &kp N7      &kp N7
                &kp N7      &kp N7      &kp N7      &kp N7      &kp N7
                &kp N7      &kp N7      &kp N7      &kp N7      &kp N7
                &kp N7      &kp N7      &kp N7      &kp N7      &kp N7
                &lt BT      &kp N7      &kp N7      &kp N7      &kp N7
            >;
        };

        BT_layer {
            bindings = <
                &kp N7      &kp N7      &kp N7      &kp N7      &kp N7
                &kp N7      &kp N7      &kp N7      &kp N7      &kp N7
                &kp N7      &kp N7      &kp N7      &kp N7      &kp N7
                &kp N7      &kp N7      &kp N7      &kp N7      &kp N7
                &kp N7      &kp C       &kp KP_N7   &kp N7      &kp N7
